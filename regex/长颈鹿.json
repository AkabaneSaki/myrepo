.anim-1 { animation: text-in 3s ease-out 9s forwards; }.anim-2 { animation: text-in 3s ease-out 10s forwards; }.anim-3 { animation: text-in 3s ease-out 11s forwards; }/* ÊòüÂÖâÊñ©Âáª: 14s ÂºÄÂßã (Á≠âËÅöÂÖâÁÅØÂÆåÂÖ®Ê∂àÂ§±Âêé) */.slash-container{position:absolute;top:0;left:0;width:100%;height:100%;z-index:15;pointer-events:none}.slash-left { position:absolute;top:50%;left:50%;width:0;height:4px;background:#fff;box-shadow:0 0 10px #fff, 0 0 20px var(--revue-gold);transform:translate(-50%,-50%) rotate(45deg);border-radius:50%;animation:slash-appear 1.5s ease-out 14s forwards; }.slash-right { position:absolute;top:50%;left:50%;width:0;height:4px;background:#fff;box-shadow:0 0 10px #fff, 0 0 20px var(--revue-gold);transform:translate(-50%,-50%) rotate(-45deg);border-radius:50%;animation:slash-appear 1.5s ease-out 14.5s forwards; }.slash-center-burst { position:absolute;top:50%;left:50%;width:10px;height:10px;background:radial-gradient(circle,#fff,var(--revue-gold),transparent);transform:translate(-50%,-50%) scale(0);animation:burst 0.5s ease-out 15.5s forwards; }/* ÂéãËΩ¥Âè∞ËØç: 16s ÂºÄÂßã */.anim-4 { animation: text-boom 0.8s cubic-bezier(.17,.88,.32,1.27) 16s forwards, text-glow 2.5s ease-in-out 16.8s infinite; }/* ÊòüÂ∞ò: 16s ÂºÄÂßã (Êó†ÈôêÂæ™ÁéØ) */.stardust{position:absolute;top:0;left:0;width:100%;height:100%;background-image:radial-gradient(2px 2px at 20% 30%,#fff,transparent),radial-gradient(2px 2px at 80% 60%,var(--revue-gold),transparent);background-size:200px 200px;z-index:12;animation:sparkle 10s linear 16s infinite}/* === ÂÖ≥ÈîÆÂ∏ßÂä®Áîª === */@keyframes curtain-l { 0% {transform:scaleX(1)} 100% {transform:scaleX(0.05)} }@keyframes curtain-r { 0% {transform:scaleX(1)} 100% {transform:scaleX(0.05)} }/* ÈáçÂÜôÁöÑËÅöÂÖâÁÅØÂä®ÁîªÔºöÂ∑¶ -> Âè≥ -> Âõû‰∏≠ -> ‰∏ãÊ≤âÊ∂àÂ§± */@keyframes spot-move { 0% { opacity:0; transform:translateX(-30%) rotate(-15deg); } 20% { opacity:1; } 40% { transform:translateX(30%) rotate(15deg); } /* ÂéªÂè≥Ëæπ */ 70% { transform:translateX(