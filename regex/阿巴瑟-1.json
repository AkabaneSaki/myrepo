ackground-size: 20px 34px;\n  background-image: radial-gradient(circle at 50% 50%, rgba(110, 0, 170, 0.05) 0%, transparent 80%), linear-gradient(60deg, rgba(110,0,170,0.08) 25%, transparent 25.5%, transparent 75%, rgba(110,0,170,0.08) 75%), linear-gradient(120deg, rgba(110,0,170,0.08) 25%, transparent 25.5%, transparent 75%, rgba(110,0,170,0.08) 75%);\n}\n.zerg-bg-pulse {\n  position: absolute; top: 0; left: 0; right: 0; bottom: 0;\n  background: radial-gradient(circle at center, var(--z-dim) 0%, transparent 70%);\n  opacity: 0.5; z-index: 0;\n  animation: zerg-breathe 5s infinite ease-in-out;\n}\n.zerg-content { position: relative; z-index: 2; line-height: 1.5; text-shadow: 0 1px 1px #000; }\n.zerg-footer {\n  background: #050008; padding: 3px 10px; border-top: 1px solid #330055;\n  display: flex; align-items: center; gap: 8px; font-size: 0.65em; color: var(--z-secondary);\n}\n.z-progress-track { flex-grow: 1; height: 3px; background: #220033; }\n.z-progress-bar { height: 100%; width: 0%; background: var(--z-primary); box-shadow: 0 0 5px var(--z-primary); animation: biomass-load 4s infinite alternate; }\n@keyframes zerg-breathe { 0%, 100% { opacity: 0.2; } 50% { opacity: 0.5; } }\n@keyframes scan-line { 0% { opacity:0; transform: translateX(-50%); } 50% { opacity:1; } 100% { opacity:0; transform: translateX(50%); } }\n@keyframes biomass-load { 0% { width: 0%; opacity: 0.7; } 100% { width: 100%; opacity: 1; } }\n@keyframes spin-dna { 0% { transform: rotateY(0deg); } 100% { transform: rotateY(360deg); } }\n</style>",
